{
  "name": "vite-plugin-react-og-image",
  "version": "0.1.0",
  "description": "Vite plugin for generating Open Graph images with Satori",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "format": "biome check --write .",
    "lint": "biome check .",
    "prepublishOnly": "pnpm build",
    "build": "tsup"
  },
  "keywords": [
    "vite",
    "plugin",
    "react",
    "og image",
    "og:image",
    "satori"
  ],
  "homepage": "https://github.com/sunya9/vite-plugin-react-og-image",
  "repository": {
    "type": "git",
    "url": "https://github.com/sunya9/vite-plugin-react-og-image.git"
  },
  "bugs": {
    "url": "https://github.com/sunya9/vite-plugin-react-og-image/issues"
  },
  "author": {
    "name": "sunya9",
    "url": "https://github.com/sunya9"
  },
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.8",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "vite": ">=5.0.0"
  },
  "dependencies": {
    "@vercel/og": "^0.6.8"
  },
  "packageManager": "pnpm@10.12.1",
  "engines": {
    "node": ">=20"
  }
}
